      <endpoint address="http://ninkasi.gil.test/WebServiceJDE.asmx" binding="customBinding" bindingConfiguration="Service1Soap12" contract="JDEdwardsService.Service1Soap" name="Service1Soap12" />

      
    </client>
  </system.serviceModel>


